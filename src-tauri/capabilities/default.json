{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "notification:default",
    "websocket:default",
    "deep-link:default",
    "dialog:default",
    "fs:default",
    "fs:allow-exists",
    "fs:allow-read-file",
    "fs:allow-write-file",
    "fs:allow-read-dir",
    "fs:allow-mkdir",
    "fs:allow-remove",
    "fs:allow-read-text-file",
    "fs:allow-write-text-file",
    "fs:scope-appdata-recursive"
  ]
}